<?xml version="1.0" encoding="UTF-8"?>
<!-- Settings placeholder generated for Cambalache editing -->
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkBox" id="settings_root">
    <property name="orientation">vertical</property>
    <property name="spacing">16</property>
    <property name="margin-start">32</property>
    <property name="margin-end">32</property>
    <property name="margin-top">32</property>
    <property name="margin-bottom">32</property>
    <property name="hexpand">true</property>
    <property name="vexpand">true</property>
    <property name="css-classes">settings-page</property>
    <child>
      <object class="GtkButton" id="settings_close_button">
        <property name="label">Back to Live View</property>
        <property name="halign">start</property>
        <property name="css-classes">control-button</property>
      </object>
    </child>
    <child>
      <object class="GtkLabel" id="settings_title">
        <property name="label">Settings</property>
        <property name="halign">start</property>
        <property name="css-classes">settings-title</property>
      </object>
    </child>
    <child>
      <object class="GtkFrame" id="settings_placeholder">
        <property name="hexpand">true</property>
        <property name="vexpand">true</property>
        <property name="label-xalign">0.0</property>
        <property name="label">Coming Soon</property>
        <child>
          <object class="GtkBox" id="settings_placeholder_box">
            <property name="orientation">vertical</property>
            <property name="spacing">8</property>
            <property name="margin-start">12</property>
            <property name="margin-end">12</property>
            <property name="margin-top">12</property>
            <property name="margin-bottom">12</property>
            <child>
              <object class="GtkLabel" id="settings_placeholder_label">
                <property name="label">Touch-friendly settings panels will live here.</property>
                <property name="wrap">true</property>
                <property name="justify">center</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
