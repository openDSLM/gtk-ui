<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="adw" version="1.0"/>
  <object class="AdwApplicationWindow" id="main_window">
    <property name="default-width">1280</property>
    <property name="default-height">720</property>
    <property name="title">openDSLM â€“ Live Preview + RAW (DNG)</property>
    <child>
      <object class="GtkBox" id="main_root">
        <property name="hexpand">true</property>
        <property name="vexpand">true</property>
        <property name="orientation">vertical</property>
        <child>
          <object class="AdwHeaderBar" id="top_header">
            <property name="title-widget">
              <object class="AdwViewSwitcherTitle" id="view_switcher_title">
                <property name="title">openDSLM</property>
                <property name="stack">page_stack</property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="GtkOverlay" id="main_root_overlay">
            <property name="hexpand">true</property>
            <property name="vexpand">true</property>
            <child>
              <object class="AdwViewStack" id="page_stack">
                <property name="hexpand">true</property>
                <property name="vexpand">true</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwViewSwitcherBar" id="view_switcher_bar">
            <property name="stack">page_stack</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
